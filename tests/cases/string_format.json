{
  "$id": "https://example.com/string_format.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "String format",
  "type": "object",
  "properties": {
    "when": {
      "type": "string",
      "format": "date-time"
    },
    "who": {
      "type": "string",
      "format": "email"
    },
    "hostname": {
      "type": "string",
      "format": "hostname"
    },
    "IP address": {
      "type": "string",
      "format": "ipv6"
    }
  }
}